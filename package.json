    "lodash": "~4.17.11",
    "node-iptc": "~1.0.5",
    "num2fraction": "~1.2.2",
    "prettier": "^1.16.0",
    "react-styleguidist": "^9.0.0-beta4",
    "remark-cli": "~6.0.1",
    "remark-lint": "~6.0.4",
    "richtypo": "^4.0.0-beta.0",
    "richtypo-rules-en": "^1.0.0-beta.0",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.2",
    "unist-util-visit": "~1.4.0",
    "utf8": "~3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/sapegin/birdwatcher.git"
  },
  "lint-staged": {
    "*.{js,md}": [
      "prettier --write",
      "git add"
