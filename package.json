{
  "name": "morning-photos",
  "version": "0.0.0",
  "description": "Artem Sapegin Photography",
  "author": "Artem Sapegin <artem@sapegin.ru> (http://sapegin.me/)",
  "license": "SEE LICENSE IN <Readme.md>",
  "private": true,
  "scripts": {
    "pretest": "npm run lint",
    "test": "npm run lint:md && npm run format:md",
    "posttest": "npm run format",
    "start": "tamia server --lang en",
    "start:ru": "tamia server --lang ru",
    "build": "babel-node src",
    "build:watch": "chokidar templates source config src -c 'babel-node src'",
    "bundle": "tamia bundle",
    "lint": "eslint . --cache --fix --ext .js,.jsx",
    "lint:md": "remark --quiet source",
    "format:md": "prettier --write 'source/**/*.md'",
    "sizes": "babel-node src/sizes",
    "format": "prettier --write \"**/*.{js,md}\"",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "has-touch": "~1.0.1",
    "social-likes-next": "~1.1.0",
    "sweet-scroll": "~1.0.4",
    "swiper": "~3.3.1",
    "tamia": "~2.0.6",
    "tamia-ajax-form": "~0.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "~7.2.3",
    "babel-plugin-transform-react-jsx": "~6.24.1",
    "babel-preset-tamia": "~6.8.0",
    "bem-cn": "~2.1.3",
    "chokidar-cli": "^1.2.1",
    "eslint": "~5.11.0",
    "eslint-config-tamia": "~6.0.2",
    "exif-parser": "~0.1.9",
    "fledermaus": "^7.1.0",
    "fs-extra": "~3.0.1",
    "glob": "~7.1.2",
    "husky": "^1.2.1",
    "lint-staged": "^8.1.0",
    "lodash": "~4.17.11",
    "minimist": "~1.2.0",
    "node-iptc": "~1.0.4",
    "num2fraction": "~1.2.2",
    "prettier": "^1.15.3",
    "remark-cli": "~2.0.0",
    "remark-lint": "~5.1.0",
    "richtypo": "~1.0.1",
    "sharp": "~0.18.2",
    "tamia-build": "~0.4.0",
    "unist-util-visit": "~1.1.3",
    "utf8": "~2.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/sapegin/birdwatcher.git"
  },
  "lint-staged": {
    "*.{js,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  }
}
