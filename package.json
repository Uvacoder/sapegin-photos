{
  "name": "morning-photos",
  "description": "Artem Sapegin Photography",
  "author": "Artem Sapegin <artem@sapegin.ru> (http://sapegin.me/)",
  "license": "SEE LICENSE IN <Readme.md>",
  "private": true,
  "scripts": {
    "start": "gatsby develop",
    "test": "npm run lint:js && npm run lint:css && npm run lint:md",
    "posttest": "npm run format",
    "lint:js": "eslint . --cache --fix",
    "lint:css": "stylelint 'src/**/*.js'",
    "lint:md": "remark --quiet content",
    "format": "prettier --write '**/*.{js,md}'",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "engines": {
    "node": ">=8.9"
  },
  "browserslist": [
    ">1%",
    "last 1 version",
    "Firefox ESR",
    "not dead"
  ],
  "dependencies": {
    "@emotion/core": "^10.0.6",
    "@emotion/styled": "^10.0.6",
    "emotion-theming": "^10.0.6",
    "react": "16.8.0-alpha.0",
    "react-dom": "16.8.0-alpha.0",
    "react-group": "^2.0.0",
    "react-helmet": "^5.2.0",
    "react-photo-gallery": "^6.3.0",
    "rehype-react": "^3.1.0",
